<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dendytrinanda.twicert">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Twicert"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="Twicert">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gejala"
            android:label="Twicert">
            <intent-filter>
                <action android:name="com.dendytrinanda.twicert.GEJALA" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name="diagnosa"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.dendytrinanda.twicert.DIAGNOSA" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="Profile"
            android:screenOrientation="portrait" />
    </application>

</manifest>